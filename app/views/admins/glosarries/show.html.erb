<div class="row">
  <%= breadcrumb do %>
    <%= breadcrumb_item %>
    <%= breadcrumb_item 'Glosarries', admins_glosarries_path %>
    <%= breadcrumb_item 'Preview' %>
  <% end %>
</div>

<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header"><%= @glosarry.title %></h1>
  </div>
</div>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-body">
        <%= @glosarry.description %>
      </div>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-body">
        <%= link_to admins_glosarries_path, :class => 'btn btn-primary' do %>
          <span class="glyphicon glyphicon-menu-left"></span>&nbsp;Back
        <% end %>
        <%= link_to edit_admins_glosarry_path(@glosarry), :class => 'btn btn-info' do %>
          <span class="glyphicon glyphicon-pencil"></span>&nbsp;Edit
        <% end %>
      </div>
    </div>
  </div>
</div>
